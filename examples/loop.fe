main : int () => {
	
	num: int = 3;

	ret: int = pow(num, 5);

	show(num);
	show(ret);
	
	return 0;
}

pow: int (base: int, exponent: int) => {
	if (exponent == 0) {
		return 1;
	} else if (base == 1) {
		return 1;
	} else if (base == 0) {
		return 0;
	}

	res: int = base;

	while ((--exponent) != 0) {
		res = res * base;
		break;
	}
	return res;
}
