main : int () => {
	
	squared: int;
	number: int = 3;
	squared = square(number);
	
	if (squared == 9) {
		put("3^2");
	} else if (squared == number) {
		put("1^2");
	} else {
		put("Not 3^2 or 1^2");
	}

	show(number);
	show(squared);
	
	return 0;
}

pow : int (base: int, exponent: int) => {
	return base * base;
}

square: int (num: int) => {
	return pow(num, 2);
}
